<!DOCTYPE html>
<html>
  <head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title></title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1">
     <% include frontend.ejs %>
  </head>
  <body>
	 <% include navbar.ejs %>
     <div style="padding-top:75px;">
				<div class="container">
					<div style="padding-top:75px;"  class="col-md-12">
						<h2><%= usersNumber %> &nbsp;Membri</h2> <br/> 
					</div>
					 <div class="row" >
					 <table class="table" >
						 <tr style="height:50px;">
						 <th  style="width: 190px;" >Nume</th>
						 <th>Email</th>
						 <th>Telefon</th>
						 <th>Job</th>
						 <th>Pozitie</th>
						 <th>Data <br/>ultimei plati</th>
						  <th>Activitate</th>
						 <th>Rezervari<br/>Evenimente </th>
						 <th>Participare <br/>Evenimente</th>
						 
						
						  <!--<th>Fisa degustare</th>-->
						 <th style="width: 100px;" ></th>
						 </tr>
						 <% Object.keys(data).forEach(function (id) {
                    var user= data[id]; %>		
						 <tr>
						 <td><%=user.username%> <%=user.lastname%></td>
						 <td><%=user.email%></td>
						 <td><%=user.phone%></td>
						 <td><%=user.job%></td>
						 <td><%=user.position%></td>
						 <td><%=user.LastFeePayDate%></td>
						  <td><%=user.activeAccount%></td>
							<td>
							<a href="#" >Vezi Rezervarile</a>
						 </td>
						<td>
							<a href="#" class="btn btn-default glyphicon glyphicon-eye-open" style="color:#8B0000;"></a>
						 </td>						 
					   <td>
							<a href="/members/edit/<%= user.id %>" class="btn btn-default glyphicon glyphicon-pencil" style="color:#8B0000;" ></a>
							<a href="#" class="btn btn-default glyphicon glyphicon-trash"  style="color:#8B0000;"></a>
						 </td>
					</tr>
						   <% });%>
					 </table>
				</div>
	 </div>
				
     </div>
  </body>
</html>